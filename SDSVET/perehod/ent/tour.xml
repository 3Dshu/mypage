	<action name="lopen_st">
			set(plugin[lscreen_tint].visible, true);
			tween(plugin[lscreen_tint].alpha,.9,0.5,default);
			set(layer[lframe].visible,true);
			tween(layer[lframe].alpha,1,2);
			skin_hideskin();
		</action>
	
		<action name="lclose_st">
			tween(plugin[lscreen_tint].alpha,0,0.5,default);
			set(plugin[lscreen_tint].visible, false);
			tween(layer[lframe].alpha,0,.3,default,wait);
			set(layer[lframe].visible,false);
			skin_showskin();
		</action>
		 <action name="reset">
			tween(layer[lcc_01].y,-45%);
			tween(layer[ltext_01].x,0.5%,1);
			tween(layer[ltext_01].alpha,.8,.5);
			tween(layer[lcc_02].y,-45%);
			tween(layer[ltext_02].x,33.833%,1);
			tween(layer[ltext_02].alpha,.8,.5);
			tween(layer[lcc_03].y,-45%);
			tween(layer[ltext_03].x,67.166%,1);
			tween(layer[ltext_03].alpha,.8,.5);
			tween(layer[lscrollarea].x, -100%);
		</action>
		<action name="lreset_arrow">
			tween(layer[lcc_01].y,-45%);
			tween(layer[ltext_01].x,0.5%,1);
			tween(layer[ltext_01].alpha,.8,.5);
			tween(layer[lcc_02].y,-45%);
			tween(layer[ltext_02].x,33.833%,1);
			tween(layer[ltext_02].alpha,.8,.5);
			tween(layer[lcc_03].y,-45%);
			tween(layer[ltext_03].x,67.166%,1);
			tween(layer[ltext_03].alpha,.8,.5);
		</action>
